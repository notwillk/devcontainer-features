{
  "id": "turbo",
  "version": "1.0.1",
  "name": "turborepo",
  "dependsOn": {
    "ghcr.io/notwillk/devcontainer-features/node:1": {}
  },
  "options": {
    "version": {
      "type": "string",
      "default": "latest",
      "description": "Turbo version to install (npm i -g turbo@<version>)"
    }
  },
  "containerEnv": {
    "DO_NOT_TRACK": "1",
    "TURBO_TELEMETRY_DISABLED": "1"
  }
}
